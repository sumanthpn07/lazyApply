# LazyApply Configuration

# Brave Search API - Get yours at https://brave.com/search/api/
BRAVE_API_KEY=your_brave_api_key_here

# Notion Integration - Your database ID
NOTION_DATABASE_ID=your_notion_database_id_here

# Server Configuration
PORT=3000
NODE_ENV=development

# Rate Limiting (milliseconds)
LINKEDIN_DELAY_MIN=300000
LINKEDIN_DELAY_MAX=480000
LINKEDIN_DAILY_LIMIT=50
DEFAULT_DELAY_MIN=120000
DEFAULT_DELAY_MAX=240000
DEFAULT_DAILY_LIMIT=100

# Browser Configuration
HEADLESS=false
SLOW_MO=50
